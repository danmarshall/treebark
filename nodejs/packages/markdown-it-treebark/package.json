{
  "name": "markdown-it-treebark",
  "version": "1.0.0",
  "description": "Markdown-it plugin for treebark template rendering",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:browser && npm run build:browser:min && npm run copy:browser",
    "build:browser": "vite build --config vite.browser.config.ts",
    "build:browser:min": "vite build --config vite.browser.min.config.ts",
    "copy:browser": "node -e \"const fs=require('fs'),path=require('path'); const dir=path.join(__dirname,'../../../docs/assets'); if(!fs.existsSync(dir)) fs.mkdirSync(dir,{recursive:true}); fs.copyFileSync('browser/markdown-it-treebark-browser.js', path.join(dir,'markdown-it-treebark-browser.js')); fs.copyFileSync('browser/markdown-it-treebark-browser.min.js', path.join(dir,'markdown-it-treebark-browser.min.js'));\"",
    "clean": "rm -rf dist browser",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "markdown-it",
    "plugin",
    "treebark",
    "template",
    "safe",
    "html"
  ],
  "author": "Dan Marshall",
  "license": "MIT",
  "dependencies": {
    "treebark": "file:../treebark"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/markdown-it": "^13.0.0",
    "cross-env": "^7.0.3",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.0.0"
  },
  "peerDependencies": {
    "markdown-it": "^13.0.0 || ^14.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
