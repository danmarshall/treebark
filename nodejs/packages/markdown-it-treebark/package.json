    "@types/js-yaml": "^4.0.0",
    "js-yaml": "^4.1.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "peerDependencies": {
    "markdown-it": "^13.0.0 || ^14.0.0"
