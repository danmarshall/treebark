    "array-bind-property": {
        "label": "Array Binding: $bind to Property",
        "template": {
            "ul": {
                "$bind": "products",
                "$children": [
                    {
                        "li": "{{name}} \u2014 {{price}}"
                    }
                ]
            }
        },
        "data": {
            "products": [
                {
                    "name": "Laptop",
                    "price": "$999"
                },
                {
                    "name": "Phone",
                    "price": "$499"
                }
            ]
        }
    },
    "array-bind-dot": {
        "label": "Array Binding: $bind with \".\"",
        "template": {
            "ul": {
                "$bind": ".",
                "$children": [
                    {
                        "li": "{{name}} \u2014 {{price}}"
                    }
                ]
            }
        },
        "data": [
            {
                "name": "Laptop",
                "price": "$999"
            },
            {
                "name": "Phone",
                "price": "$499"
            }
        ]
